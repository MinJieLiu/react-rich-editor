{
  "name": "react-rich-editor",
  "version": "0.1.2",
  "description": "React Rich Text Editor",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf lib",
    "start": "better-npm-run start",
    "deploy": "npm run clean && better-npm-run prod",
    "lint": "eslint src --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node config/server.dev.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "prod": {
      "command": "babel-node config/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MinJieLiu/react-rich-editor.git"
  },
  "keywords": [
    "react",
    "editor",
    "react-editor",
    "rich-editor"
  ],
  "author": "LMY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MinJieLiu/react-rich-editor/issues"
  },
  "homepage": "https://github.com/MinJieLiu/react-rich-editor#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.25.0",
    "debug": "^2.3.2",
    "draft-js": "^0.10.0",
    "draftjs-to-html": "^0.6.2",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "immutable": "3.8.1",
    "node-sass": "^3.13.1",
    "postcss-loader": "^1.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "draft-js": "^0.10.x",
    "immutable": "3.8.x",
    "react": "0.14.x || 15.x",
    "react-dom": "0.14.x || 15.x"
  }
}
